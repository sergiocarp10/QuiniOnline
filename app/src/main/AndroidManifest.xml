<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs10.apps.quini">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_quiniela"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_quiniela"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuiniOnline">
        <activity
            android:name=".DrawerActivity"
            android:exported="false"
            android:label="@string/title_activity_drawer"
            android:theme="@style/Theme.QuiniOnline.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.QuiniOnline.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>